	dcterms:subject <https://github.com/tibonto/dfgfo/408-03> ;
	skos:definition "This is the main element to describe energy research software." ;
	schema:version "0.8";
	sh:extends schema:SoftwareApplication ;
	sh:order 1;
	sh:closed false ;
	sh:property [
